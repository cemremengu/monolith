# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/my_db

# JWT Configuration
SECRET_KEY=your-super-secret-jwt-signing-key-here
JWT_ACCESS_TOKEN_DURATION=10m # Access token duration. Should be very short ideally. (default: 10 minutes)
JWT_REFRESH_TOKEN_DURATION=7d # Refresh token duration (default: 7 days)
SESSION_TIMEOUT=24h # Session timeout (default: 0 = no timeout)

# Server
PORT=3001
ENV=development
LOG_LEVEL=debug

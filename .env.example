# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/my_db

# JWT Configuration
SECRET_KEY=your-super-secret-jwt-signing-key-here
JWT_ACCESS_TOKEN_DURATION=24      # Access token duration in hours (default: 24 hours)
JWT_REFRESH_TOKEN_DURATION=168    # Refresh token duration in hours (default: 168 hours = 7 days)

# Server
PORT=3001
ENV=development
LOG_LEVEL=debug
